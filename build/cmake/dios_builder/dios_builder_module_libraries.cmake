

# 
# 加载模块链接库
# 
macro(_dios_builder_module_link_libraries)
	# 链接库
	LIST(LENGTH DIOS_MODULE_${DIOS_BUILDER_MODULE}_LIBRARIES LEN)
	# MESSAGE(STATUS "======= TARGET_LINK_LIBRARIES(${DIOS_BUILDER_MODULE} ${DIOS_MODULE_${DIOS_BUILDER_MODULE}_LIBRARIES}) ${LEN}")
	IF(NOT LEN EQUAL 0)
		# MESSAGE(STATUS "======= TARGET_LINK_LIBRARIES(${DIOS_BUILDER_MODULE} ${DIOS_MODULE_${DIOS_BUILDER_MODULE}_LIBRARIES}) ${LEN}")
		TARGET_LINK_LIBRARIES(${DIOS_BUILDER_MODULE} ${DIOS_MODULE_${DIOS_BUILDER_MODULE}_LIBRARIES})
	ENDIF()
endmacro()